<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>syrchalis.traitvalue</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/TraitDef[defName = "Straight"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>0</li>
              </traitValues>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/TraitDef[defName = "Faithful"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>10</li>
              </traitValues>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/TraitDef[defName = "Philanderer"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>-10</li>
              </traitValues>
            </li>
          </value>
        </li>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/TraitDef[defName = "Polyamorous"]</xpath>
          <value>
            <li Class="SyrTraitValue.TraitValueExtension">
              <traitValues>
                <li>0</li>
              </traitValues>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>